package main;

public class Aluno {
	
	private String matricula;
	private String nome;
	private String curso;
	
	public Aluno(String matricula, String nome, String curso)
		throws Exception{
		if(matricula == null || matricula.equals("")){
			throw Exception ("matricula nao pode ser null nem vazia.");
		}
	}
		
	
}
