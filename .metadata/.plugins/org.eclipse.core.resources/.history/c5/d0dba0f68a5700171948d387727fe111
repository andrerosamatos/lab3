package main;

import java.util.HashSet;

public class Grupo {
	
	private String nome;
	Set<Aluno> alunos = new Hashset<>();
	public Grupo(String nome) throws Exception{
		if(nome == null || nome.equals("")){
			throw new Exception ("Nome do grupo nao pode ser null nem vazio.");
		}
		this.nome = nome;
	
		
	}
	

}
